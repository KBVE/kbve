{
	"name": "kbve.com",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"projectType": "application",
	"sourceRoot": "apps/kbve.com/src",
	"tags": [],
	"targets": {
		"build": {
			"executor": "@nxtensions/astro:build",
			"options": {}
		},
		"dev": {
			"executor": "@nxtensions/astro:dev",
			"options": {}
		},
		"preview": {
			"dependsOn": [
				{
					"target": "build",
					"projects": "self"
				}
			],
			"executor": "@nxtensions/astro:preview",
			"options": {}
		},
		"check": {
			"executor": "@nxtensions/astro:check"
		},
		"sync": {
			"executor": "@nxtensions/astro:sync"
		},
		"graph": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"chmod +x tools/scripts/public/js/build_node_graph.sh",
					"./tools/scripts/public/js/build_node_graph.sh"
				],
				"parallel": false
			}
		},
		"music": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"chmod +x tools/scripts/public/music.sh",
					"./tools/scripts/public/music.sh {args.name}"
				],
				"parallel": false
			}
		}
	}
}
