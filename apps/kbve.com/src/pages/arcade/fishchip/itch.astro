---
import Core from '../../../layouts/Core.astro';
import { AstroVe as VE } from '@kbve/astro-ve';

let _title = 'Phaser Demo';
let _description = 'This is a phaser demo!';

---

<Core title={_title} description={_description}>

    
    <section
    class={`flex-auto w-full md:w-6/12 p-5 bg-[#c0987d] border border-cyan-200 rounded-lg shadow bg-default bg-cover bg-blend-overlay`}
    style={`background-image: url('https://utfs.io/f/ae961975-ad95-4247-91cc-2c573d6fbe3e-1t0m0l.webp')`}>

        <VE widget="header:embed:phaser" scene="/embed/js/phaser/fish/TownScene.js">
            
        </VE>

    </section>


</Core>