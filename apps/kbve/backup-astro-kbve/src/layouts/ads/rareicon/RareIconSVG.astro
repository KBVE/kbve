<script is:inline>
	function setTextAnimation(
		delay,
		duration,
		strokeWidth,
		timingFunction,
		strokeColor,
		repeat
	) {
		const scopedContainer = document.querySelector('.svg-anim-scope');
		if (!scopedContainer) return;

		let paths = scopedContainer.querySelectorAll('path');
		let mode = repeat ? 'infinite' : 'forwards';

		paths.forEach((path, i) => {
			const length = path.getTotalLength();
			path.style.strokeDashoffset = `${length}px`;
			path.style.strokeDasharray = `${length}px`;
			path.style.strokeWidth = `${strokeWidth}px`;
			path.style.stroke = strokeColor;
			path.style.animation = `${duration}s svg-text-anim ${mode} ${timingFunction}`;
			path.style.animationDelay = `${i * delay}s`;
		});
	}

	document.addEventListener("DOMContentLoaded", () => {
		setTextAnimation(0.1, 2.8, 2, 'linear', '#000000', true);
	});
	document.addEventListener("astro:page-load", () => {
		setTextAnimation(0.1, 2.8, 2, 'linear', '#000000', true);
	});
</script>

<style>
	.svg-anim-scope path {
		fill: transparent;
		stroke: #000;
	}

	/* Scoped animation */
	@-webkit-keyframes svg-text-anim {
		40% {
			stroke-dashoffset: 0;
			fill: transparent;
		}
		60% {
			stroke-dashoffset: 0;
			fill: #6ed1f2;
		}
		100% {
			stroke-dashoffset: 0;
			fill: #6ed1f2;
		}
	}

	@keyframes svg-text-anim {
		40% {
			stroke-dashoffset: 0;
			fill: transparent;
		}
		60% {
			stroke-dashoffset: 0;
			fill: #6ed1f2;
		}
		100% {
			stroke-dashoffset: 0;
			fill: #6ed1f2;
		}
	}
</style>
