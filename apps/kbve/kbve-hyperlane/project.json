{
      "name": "kbve-hyperlane",
      "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/kbve/kbve-hyperlane/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/kbve-hyperlane"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/kbve-hyperlane"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/kbve-hyperlane"
      }
    },
    "run": {
      "executor": "@monodon/rust:run",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/kbve-hyperlane"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "container": {
    "executor": "nx:run-commands",
    "options": {
        "commands": [
          "./kbve.sh -nx astro-kbve:build",
          "./kbve.sh -preparehyperlane",
          "./kbve.sh -nx kbve-hyperlane:containerx"
        ],
        "parallel": false
      }
  },
    "containerx": {
      "executor": "@nx-tools/nx-container:build",
      "defaultConfiguration": "local",
      "options": {
        "engine": "docker",
        "context": "apps/kbve/kbve-hyperlane",
        "file": "apps/kbve/kbve-hyperlane/Dockerfile",
        "metadata": {
          "images": [
            "kbve/kbve-hyperlane"
          ],
          "load": true,
          "tags": [
            "1.2.5",
            "1.2"
          ]
        },
        "configurations": {
          "local": {
            "tags": [
              "1.2.5",
              "1.2"
            ],
            "push": false
          },
          "production": {
            "tags": [
              "1.2.5",
              "1.2"
            ],
            "push": true,
            "customBuildOptions": "--push",
            "cache-from": [
              "type=registry,ref=kbve/kbve-hyperlane:buildcache"
            ],
            "cache-to": [
              "type=registry,ref=kbve/kbve-hyperlane:buildcache,mode=max"
            ]
          }
        }
      }
    },
    "proto": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/kbve/kbve-hyperlane",
        "commands": [
          {
            "command": "cargo check --no-default-features",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    },
    "orb": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "pnpm nx reset",
            "forwardAllArgs": false
          },
          {
            "command": "pnpm nx container kbve-hyperlane",
            "forwardAllArgs": false
          },
          {
            "command": "docker run --env-file .env -p 3000:3000 kbve/kbve-hyperlane:1.2.5",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    },
    "orbnorun": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "pnpm nx reset",
            "forwardAllArgs": false
          },
          {
            "command": "pnpm nx container kbve-hyperlane",
            "forwardAllArgs": false
          }
        ],
        "parallel": false
      }
    },
    "stress": {
    "executor": "nx:run-commands",
    "options": {
      "commands": [
        {
          "command": "chmod +x ./tools/scripts/public/kbve-hyperlane/kbve-hyperlane-script.sh && ./tools/scripts/public/kbve-hyperlane/kbve-hyperlane-script.sh",
          "forwardAllArgs": false
        }
      ],
      "parallel": false
    }
  },
  "docker": {
    "executor": "nx:run-commands",
    "options": {
        "commands": [
          "./kbve.sh -nx astro-kbve-hyperlane:build",
          "rm -rf ./apps/kbve/kbve-hyperlane/dist/",
          "mkdir -p ./apps/kbve/kbve-hyperlane/dist/",
          "cp -a ./dist/apps/astro-discord/. ./apps/kbve/kbve-hyperlane/dist/",
          "./kbve.sh -nx kbve-hyperlane:orbnorun",
          "cd ./apps/kbve/kbve-hyperlane && docker-compose --env-file .env up --build"
        ],
        "parallel": false
      }
  }
  },
  "tags": []
}