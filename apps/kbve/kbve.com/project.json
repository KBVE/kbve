{
  "name": "kbve.com",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/kbve/kbve.com/src",
  "tags": [],
  "targets": {
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/kbve/kbve.com",
        "commands": [
          "UV_THREADPOOL_SIZE=4 NODE_OPTIONS=\"--max-old-space-size=4096\" nx exec -- astro dev"
        ],
        "parallel": false
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/kbve/kbve.com",
        "commands": [
          "UV_THREADPOOL_SIZE=4 NODE_OPTIONS=\"--max-old-space-size=4096\" nx exec -- astro build"
        ],
        "parallel": false
      }
    },
    "buildx": {
      "executor": "@nxtensions/astro:build",
      "options": {}
    },
    "devx": {
      "executor": "@nxtensions/astro:dev",
      "options": {}
    },
    "preview": {
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "executor": "@nxtensions/astro:preview",
      "options": {}
    },
    "check": {
      "executor": "@nxtensions/astro:check"
    },
    "sync": {
      "executor": "@nxtensions/astro:sync"
    },
    "graph": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "chmod +x tools/scripts/public/js/build_node_graph.sh",
          "./tools/scripts/public/js/build_node_graph.sh"
        ],
        "parallel": false
      }
    },
    "music": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "chmod +x tools/scripts/public/music.sh",
            "forwardAllArgs": false
          },
          {
            "command": "./tools/scripts/public/music.sh",
            "forwardAllArgs": true
          }
        ],
        "parallel": false
      }
    }
  }
}
