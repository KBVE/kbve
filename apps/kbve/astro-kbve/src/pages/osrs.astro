---
import Layout from 'src/layouts/core/Layout.astro';
import Supabase from 'src/layouts/core/Supabase.astro';
import { OsrsApp } from 'src/layouts/client/osrs/OsrsApp';
import MembersOnly from 'src/layouts/client/supabase/auth/MembersOnly';
---

<Layout title="OSRS Dashboard | Old School RuneScape Management">
  <Supabase />
  <MembersOnly client:only="react" />
  
  <div class="w-full h-screen p-4 bg-gradient-to-br from-slate-100 via-blue-50 to-indigo-100 dark:from-slate-900 dark:via-slate-800 dark:to-indigo-950 overflow-hidden">
    <OsrsApp client:only="react" />
  </div>
</Layout>
