<script
	is:inline
	async
	src="https://cse.google.com/cse.js?cx=partner-pub-3698214103602019:ab1srxiqkuo"
></script>

<!-- Google Search Results -->
<div
	class="relative mx-auto bg-zinc-900"
	aria-label="Search Results From Google">
	<!-- Inner Block -->
	<div
		class="absolute inset-[2px] rounded-[1.375rem] pointer-events-none z-10
             border border-[rgba(34,211,238,0.1)]
             shadow-[inset_0.5px_0.5px_1.5px_rgba(186,230,253,0.6),inset_-1px_-1px_1px_rgba(21,94,117,0.5),inset_3px_3px_6px_rgba(0,0,0,0.25)]
             translate-z-[30px] transform-style-3d">
	</div>

	<div class="gcse-searchresults-only"></div>

	<script is:inline>
		const onGoogleSearchRendered = () => {
			console.log('[CSE] Results rendered â€” applying styles');

			const apply = (selector, styles) => {
				const el = document.querySelector(selector);
				if (el instanceof HTMLElement) {
					Object.assign(el.style, styles);
				}
			};

			const applyToAll = (selector, styles) => {
				const elements = document.querySelectorAll(selector);
				elements.forEach((el) => {
					if (el instanceof HTMLElement) {
						Object.assign(el.style, styles);
					}
				});
			};

			// Main container and results styling
			apply('.gsc-results.gsc-webResult', {
				backgroundColor: '#18181b',
				borderRadius: '0.5rem',
				padding: '1rem',
				border: '1px solid #27272a',
			});

			// Google Custom Search control container
			apply('.gsc-control-cse', {
				backgroundColor: '#0c0a09 !important',
				border: 'none !important',
				padding: '0 !important',
			});

			apply('.gsc-control-cse-en', {
				backgroundColor: '#0c0a09 !important',
				border: 'none !important',
			});

			// Individual search result items
			applyToAll('.gsc-webResult.gsc-result', {
				backgroundColor: '#0c0a09',
				borderRadius: '0.375rem',
				marginBottom: '1rem',
				padding: '1rem',
				border: '1px solid #3f3f46',
			});

			// Search result titles
			applyToAll('.gs-title a', {
				color: '#22d3ee !important',
				textDecoration: 'none',
				fontSize: '1.125rem',
				fontWeight: '600',
			});

			// Search result snippets/descriptions
			applyToAll('.gs-snippet', {
				color: '#d4d4d8 !important',
				fontSize: '0.875rem',
				lineHeight: '1.5',
				marginTop: '0.5rem',
			});

			// URLs styling
			applyToAll('.gs-visibleUrl', {
				color: '#71717a !important',
				fontSize: '0.75rem',
			});

			applyToAll('.gs-visibleUrl-short', {
				color: '#a1a1aa !important',
			});

			applyToAll('.gs-visibleUrl-long', {
				color: '#71717a !important',
			});

			applyToAll('.gs-visibleUrl-breadcrumb', {
				color: '#71717a !important',
			});

			// Pagination styling
			apply('.gsc-cursor-box.gs-bidi-start-align', {
				backgroundColor: '#18181b',
				borderRadius: '0.5rem',
				padding: '1rem',
				marginTop: '1rem',
				border: '1px solid #3f3f46',
			});

			applyToAll('.gsc-cursor-page', {
				backgroundColor: '#27272a !important',
				color: '#d4d4d8 !important',
				border: '1px solid #3f3f46 !important',
				borderRadius: '0.375rem !important',
				padding: '0.5rem 0.75rem !important',
				margin: '0 0.25rem !important',
				cursor: 'pointer !important',
				transition: 'all 0.2s ease !important',
			});

			applyToAll('.gsc-cursor-current-page', {
				backgroundColor: '#22d3ee !important',
				color: '#0c0a09 !important',
				fontWeight: '600 !important',
			});

			// Expansion area styling
			apply('.gsc-expansionArea', {
				backgroundColor: '#18181b',
				borderTop: '1px solid #3f3f46',
				borderRadius: '0.5rem',
			});

			// Google branding area
			apply('.gcsc-more-maybe-branding-root', {
				backgroundColor: '#18181b',
				borderRadius: '0.5rem',
				padding: '1rem',
				marginTop: '1rem',
				border: '1px solid #3f3f46',
			});

			// Google "find more" link styling
			applyToAll('.gcsc-find-more-on-google', {
				color: '#71717a !important',
				fontSize: '0.875rem',
			});

			applyToAll('.gcsc-find-more-on-google-text', {
				color: '#a1a1aa !important',
			});

			applyToAll('.gcsc-find-more-on-google-query', {
				color: '#22d3ee !important',
				fontWeight: '600',
			});

			// Table result styling for better layout
			applyToAll('.gsc-table-result', {
				backgroundColor: 'transparent',
			});

			applyToAll('.gsc-table-cell-snippet-close', {
				backgroundColor: 'transparent',
			});

			// URL top and bottom sections
			applyToAll('.gsc-url-top', {
				marginBottom: '0.5rem',
			});

			applyToAll('.gsc-url-bottom', {
				marginTop: '0.5rem',
			});

			// Hide rich snippets if they appear
			applyToAll('.gs-richsnippet-box', {
				display: 'none !important',
			});

			// Style the main web result container
			applyToAll('.gs-webResult.gs-result', {
				backgroundColor: 'transparent',
				border: 'none',
				margin: '0',
				padding: '0',
			});

			// Hover effects for pagination
			document.querySelectorAll('.gsc-cursor-page').forEach((el) => {
				if (el instanceof HTMLElement && !el.classList.contains('gsc-cursor-current-page')) {
					el.addEventListener('mouseenter', () => {
						el.style.backgroundColor = '#3f3f46';
						el.style.color = '#f4f4f5';
					});
					el.addEventListener('mouseleave', () => {
						el.style.backgroundColor = '#27272a';
						el.style.color = '#d4d4d8';
					});
				}
			});

			// Hover effects for search result titles
			document.querySelectorAll('.gs-title a').forEach((el) => {
				if (el instanceof HTMLElement) {
					el.addEventListener('mouseenter', () => {
						el.style.color = '#67e8f9';
					});
					el.addEventListener('mouseleave', () => {
						el.style.color = '#22d3ee';
					});
				}
			});
		};

		window.__gcse = window.__gcse || {};
		window.__gcse.searchCallbacks = {
			web: {
				rendered: onGoogleSearchRendered,
			},
			image: {
				rendered: onGoogleSearchRendered,
			},
		};
	</script>
</div>
