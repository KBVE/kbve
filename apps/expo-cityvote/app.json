{
  "expo": {
    "name": "expo-cityvote",
    "slug": "expo-cityvote",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme":"cityvote",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kbve.expo-cityvote"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.kbve.cityvote"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "outpuit": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments" : {
      "tsconfigPaths": true,
      "typeRoutes": true
    }
  }
}
