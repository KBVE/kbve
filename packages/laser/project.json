{
  "name": "laser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/laser/src",
  "projectType": "library",
  "release": {
    "version": {
      "generatorOptions": {
        "packageRoot": "packages/laser",
        "currentVersionResolver": "disk"
      }
    }
  },
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/laser",
        "main": "packages/laser/src/index.ts",
        "tsConfig": "packages/laser/tsconfig.lib.json",
        "assets": ["packages/laser/*.md"]
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/packages/laser"
      }
    }
  }
}
