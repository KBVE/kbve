{
  "name": "soul",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "packages/rust/soul/src",
  "targets": {
    "build": {
      "executor": "@monodon/rust:build",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_soul"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "test": {
      "executor": "@monodon/rust:test",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_soul"
      },
      "configurations": {
        "production": {
          "release": true
        }
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx test rust_soul"],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@monodon/rust:lint",
      "outputs": [
        "{options.target-dir}"
      ],
      "options": {
        "target-dir": "dist/target/rust_soul"
      }
    }
  },
  "tags": []
}
